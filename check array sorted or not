#include<iostream>
using namespace std;
int main()
{
    bool flag = false;
    int arr[] = {2,44,98};
    int n = sizeof(arr)/sizeof(arr[0]);
    for(int i=1;i<n;i++)
    {
        if(arr[i]>=arr[i-1])
        {
            flag = true;
            
        }
        else
        {
            flag = false;
        }
        
    }
    if(flag == true)
    {
        cout<<"1";
    }
    else{
        cout<<"0";
    }
    
    return 0;
}
